<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_bg"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".Presentacion.MainActivity">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="1dp"
        android:gravity="center"
        android:text="Menu"
        android:textAlignment="center"
        android:textColor="@color/color_white"
        android:textColorLink="#FFFFFF"
        android:textSize="35sp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="516dp"
        android:layout_margin="40dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingBottom="20dp">

        <Button
            android:id="@+id/btCliente"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_margin="2dp"
            android:backgroundTint="#181B3A"
            android:drawableRight="@drawable/cliente"
            android:insetBottom="@dimen/mtrl_btn_inset"
            android:text="Cliente"
            android:textAlignment="center"
            android:textColor="@color/color_white"
            android:textColorHint="#FFFFFF"
            app:cornerRadius="5dp"
            app:iconPadding="@dimen/mtrl_btn_text_btn_icon_padding"
            app:iconTint="@color/color_light_gray"
            app:strokeColor="@color/color_white"
            app:strokeWidth="@dimen/mtrl_btn_stroke_size" />

        <Button
            android:id="@+id/btRepartidor"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:backgroundTint="#181B3A"
            android:drawableRight="@drawable/repartidor"
            android:insetBottom="@dimen/mtrl_btn_inset"
            android:text="Repartidor"
            android:textAlignment="center"
            android:textColor="@color/color_white"
            app:cornerRadius="5dp"
            app:iconPadding="@dimen/mtrl_btn_text_btn_icon_padding"
            app:iconTint="@color/color_light_gray"
            app:strokeColor="@color/color_white"
            app:strokeWidth="@dimen/mtrl_btn_stroke_size" />

        <Button
            android:id="@+id/btProducto"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:backgroundTint="#181B3A"
            android:drawableRight="@drawable/caja"
            android:insetBottom="@dimen/mtrl_btn_inset"
            android:text="Producto"
            android:textAlignment="center"
            android:textColor="@color/color_white"
            app:cornerRadius="5dp"
            app:iconPadding="@dimen/mtrl_btn_text_btn_icon_padding"
            app:iconTint="@color/color_light_gray"
            app:strokeColor="@color/color_white"
            app:strokeWidth="@dimen/mtrl_btn_stroke_size" />

        <Button
            android:id="@+id/btCatalogo"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:backgroundTint="#181B3A"
            android:drawableRight="@drawable/documento"
            android:insetBottom="@dimen/mtrl_btn_inset"
            android:text="Catalogo"
            android:textAlignment="center"
            android:textColor="@color/color_white"
            android:textColorHighlight="#FFFFFF"
            app:cornerRadius="5dp"
            app:iconPadding="@dimen/mtrl_btn_text_btn_icon_padding"
            app:iconTint="@color/color_light_gray"
            app:strokeColor="@color/color_white"
            app:strokeWidth="@dimen/mtrl_btn_stroke_size" />

        <Button
            android:id="@+id/btUbicacion"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:backgroundTint="#181B3A"
            android:drawableRight="@drawable/map"
            android:insetBottom="@dimen/mtrl_btn_inset"
            android:text="Ubicacion"
            android:textAlignment="center"
            android:textColor="@color/color_white"
            android:textColorHint="#FFFFFF"
            app:cornerRadius="5dp"
            app:iconPadding="@dimen/mtrl_btn_text_btn_icon_padding"
            app:iconTint="@color/color_light_gray"
            app:layout_constraintEnd_toEndOf="parent"
            app:strokeColor="@color/color_white"
            app:strokeWidth="@dimen/mtrl_btn_stroke_size" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="328dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        </LinearLayout>

        <Button
            android:id="@+id/btProforma"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:backgroundTint="#181B3A"
            android:drawableRight="@drawable/nota"
            android:insetBottom="@dimen/mtrl_btn_inset"
            android:text="Proforma"
            android:textAlignment="center"
            android:textColor="@color/color_white"
            android:textColorHint="#FFFFFF"
            app:cornerRadius="5dp"
            app:iconPadding="@dimen/mtrl_btn_text_btn_icon_padding"
            app:iconTint="@color/color_light_gray"
            app:strokeColor="@color/color_white"
            app:strokeWidth="@dimen/mtrl_btn_stroke_size" />

        <Button
            android:id="@+id/btEntrega"
            style="?attr/materialButtonOutlinedStyle"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:backgroundTint="#181B3A"
            android:drawableRight="@drawable/envio"
            android:text="Lista de Entrega"
            android:textAlignment="center"
            android:textColor="@color/color_white"
            app:cornerRadius="5dp"
            app:iconTint="@color/color_light_gray"
            app:strokeColor="@color/color_white" />

        <TextView
            android:layout_width="194dp"
            android:layout_height="wrap_content"
            android:text=" Emprendedor v3"
            android:textAlignment="center"
            android:textColor="@color/color_white"
            android:textSize="20sp"
            android:textStyle="bold" />

    </LinearLayout>

</LinearLayout>